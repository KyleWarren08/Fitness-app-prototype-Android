<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#999999"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="431dp"
        android:layout_height="56dp"
        android:background="@drawable/rectangle_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/GoalsRectangle"
        android:layout_width="360dp"
        android:layout_height="243dp"
        android:background="@drawable/rectangle_bar"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.225" />

    <TextView
        android:id="@+id/txtProfile"
        android:layout_width="74dp"
        android:layout_height="38dp"
        android:fontFamily="sans-serif-medium"
        android:text="Home"
        android:textColor="#fc363b"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtGoals"
        android:layout_width="74dp"
        android:layout_height="38dp"
        android:fontFamily="sans-serif-medium"
        android:text="Goals"
        android:textColor="#fc363b"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.039" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="429dp"
        android:layout_height="54dp"
        android:background="@drawable/rectangle_bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnHome"
        android:layout_width="84dp"
        android:layout_height="43dp"
        android:background="@color/button_color"
        android:text="Home"
        android:textColor="#EFDADA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.048"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.588" />

    <Button
        android:id="@+id/btnWeight"
        android:layout_width="84dp"
        android:layout_height="43dp"
        android:background="@color/button_color"
        android:text="Weight"
        android:textColor="#EFDADA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.588" />

    <Button
        android:id="@+id/btnCalories"
        android:layout_width="84dp"
        android:layout_height="43dp"
        android:background="@color/button_color"
        android:text="Calories"
        android:textColor="#EFDADA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.349"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.588" />

    <Button
        android:id="@+id/btnProfile"
        android:layout_width="84dp"
        android:layout_height="43dp"
        android:background="@color/button_color"
        android:text="Profile"
        android:textColor="#EFDADA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.945"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.588" />

    <ImageButton
        android:id="@+id/btnSettings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toEndOf="@+id/txtHome"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.307"
        app:srcCompat="@drawable/ic_blacksettings" />

    <TextView
        android:id="@+id/txtDailyCalorieIntake"
        android:layout_width="176dp"
        android:layout_height="29dp"
        android:text="Daily Calorie Intake:"
        android:textColor="#EFDADA"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.439" />

    <TextView
        android:id="@+id/outDailyCalorieIntake"
        android:layout_width="59dp"
        android:layout_height="30dp"
        android:textColor="#EFDADA"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.664"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.436" />

    <TextView
        android:id="@+id/outCalorie"
        android:layout_width="59dp"
        android:layout_height="30dp"
        android:text="Cal"
        android:textColor="#EFDADA"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.86"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.436" />

    <TextView
        android:id="@+id/outWeight2"
        android:layout_width="59dp"
        android:layout_height="30dp"
        android:textColor="#EFDADA"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.328"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.802" />

    <TextView
        android:id="@+id/outUniteWeight2"
        android:layout_width="59dp"
        android:layout_height="30dp"
        android:textColor="#EFDADA"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.802" />

    <TextView
        android:id="@+id/outLoseOrGain"
        android:layout_width="110dp"
        android:layout_height="30dp"
        android:textColor="#EFDADA"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.868"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.802" />

    <TextView
        android:id="@+id/txtWeightGoal"
        android:layout_width="77dp"
        android:layout_height="29dp"
        android:text="Weight:"
        android:textColor="#EFDADA"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/GoalsRectangle"
        app:layout_constraintEnd_toEndOf="@+id/GoalsRectangle"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/GoalsRectangle"
        app:layout_constraintTop_toTopOf="@+id/GoalsRectangle"
        app:layout_constraintVertical_bias="0.799" />


</androidx.constraintlayout.widget.ConstraintLayout>